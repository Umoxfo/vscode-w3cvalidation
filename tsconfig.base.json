{
    "compileOnSave": true,
    "compilerOptions": {
        "plugins": [
            {
                "name": "typescript-tslint-plugin"
            }
        ],
        "module": "commonjs",
        "target": "es2017",
        "lib": [
            "es2017"
        ],
        "incremental": true,
        "sourceMap": true,
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    }
}
