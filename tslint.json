{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "object-literal-sort-keys": [
            true,
            "match-declaration-order",
            "shorthand-first"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-import-side-effect": true,
        "prefer-while": true,
        "promise-function-async": true
    }
}
